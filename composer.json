{
    "name": "bobbybouwmann/legacy-encrypter",
    "description": "The legacy version of the Laravel mcrypt encrypter (for 5.1 - 5.3).",
    "license": "MIT",
    "authors": [
        {
            "name": "Bobby Bouwmann",
            "email": "bobbybouwmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*",
        "paragonie/random_compat": "~1.4|~2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\LegacyEncrypter\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
